@model IEnumerable<ProjetFinal.Models.Fonction>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nom)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NombreLignesDeCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DernierUpdate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdBibliothequeNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NombreLignesDeCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DernierUpdate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdBibliothequeNavigation.IdBibliotheque)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdFonction">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdFonction">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdFonction">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
